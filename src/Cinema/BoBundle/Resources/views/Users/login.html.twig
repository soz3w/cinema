{% extends "CinemaBoBundle::layout.html.twig" %}
{% block content %}
    {% if error %}
        <div>{{ error.message }}</div>
    {% endif %}cd
<form action="{{ path('cinema_bo_login_check') }}" method="post">
    <div class="form-group">
        <label class="form-label" for="login"> login</label>
        <input name="_username" value="{{ last_username }}" type="text" class="form-control"/><br/>

    </div>
    <div class="form-group">
        <label class="form-label" for="login"> password</label>
        <input name="_password" type="password" class="form-control"/><br/>

    </div>

    <input type="submit"/>
</form>
{% endblock content %}